{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./extension",
    "noImplicitAny": true,
    "strict": true,
    "module": "ES6",
    "target": "ES6",
    "jsx": "react",
    "allowJs": true,
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["chrome"]
  }
}
