{
  "compilerOptions": {
    "outDir": "./extension",
    "noImplicitAny": true,
    "strict": true,
    "module": "ES6",
    "target": "ES6",
    "jsx": "react",
    "allowJs": true,
    "lib": ["es2019", "dom"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
