{
  "manifest_version": 2,
  "name": "SVG Gobbler",
  "version": "1.19",
  "description": "Download SVGs from the web",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    },
    "default_title": "SVG Gobbler"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab", "tabs"]
}
